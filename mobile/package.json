{
  "name": "GiftKeeper",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lintfix": "$NVM_DIR/versions/node/v6.10.1/bin/eslint --fix app/**/*.js",
    "format": "prettier-standard app/**/*.js && npm run lintfix",
    "emu-droid": "emulator -avd mydevice",
    "remotedev-start": "remotedev --hostname=localhost --port=8080",
    "reset": "npm start -- --reset-cache",
    "reset-all": "watchman watch-del-all && rm -rf node_modules/ && npm cache clean && npm install && npm start -- --reset-cache",
    "1234postinstall": "rndebugger-open",
    "emu-ios": "xcrun instruments -w 'iPhone 5s (10.2)'",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "jest": "jest",
    "test": "mocha --growl --require babel-polyfill ./test/mocha-setup.js --compilers js:babel-core/register --require mocha-clean test",
    "node-inspector": "node-inspector ./test & open 'http://127.0.0.1:8080/?port=5858'",
    "testwatch": "npm run test -- --watch",
    "testdb": "npm run node-inspector & npm run test -- --debug-brk",
    "testdbw": "npm run testdb -- --watch",
    "bundle": "react-native bundle --dev false --entry-file index.ios.js --bundle-output ios/main.jsbundle --platform ios",
    "validate-plist": "plutil ios/GiftKeeper/Info.plist",
    "validate-pbxproj": "kin ios/GiftKeeper.xcodeproj/project.pbxproj",
    "gen-icons": "yo rn-toolbox:assets --icon img/app_icon_blue.png",
    "gen-splash": "yo rn-toolbox:assets --splash img/splash.png",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@expo/vector-icons": "^6.2.2",
    "color": "^1.0.3",
    "json-loader": "^0.5.4",
    "moment": "^2.15.2",
    "moment-duration-format": "^1.3.0",
    "native-base": "2.3.5",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-native": "^0.51.0",
    "react-native-cached-image": "^1.3.3",
    "react-native-drawer": "^2.5.0",
    "react-native-fbsdk": "~0.6.0",
    "react-native-fetch-blob": "~0.10.0",
    "react-native-scrollable-tab-view": "~0.7.2",
    "react-native-swipeout": "^2.3.3",
    "react-native-vector-icons": "^4.0.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "sqlite": "^2.3.0",
    "uuid-js": "^0.7.5"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-eslint": "^8.1.1",
    "babel-polyfill": "^6.16.0",
    "babel-preset-react-native": "1.9.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "chai-http": "^3.0.0",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-jsx": "^4.0.2",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mocha": "^3.1.2",
    "mocha-clean": "^1.0.0",
    "mongodb": "^2.2.11",
    "prettier-standard": "^8.0.0",
    "react-native-debugger-open": "0.3.5",
    "react-native-mock": "^0.2.7",
    "redux-devtools-extension": "2.13.0",
    "redux-mock-store": "1.2.1",
    "remote-redux-devtools": "0.5.7",
    "remotedev-server": "0.3.0-beta-3",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier-standard",
      "git add"
    ]
  }
}
